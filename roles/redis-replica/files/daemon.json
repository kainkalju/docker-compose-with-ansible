{
  "live-restore": true,
	"storage-driver": "overlay2",
	"mtu": 1460,
  "log-driver": "local",
  "log-opts": {
    "compress": "true",
    "max-file": "5",
    "max-size": "100m",
    "tag": "{{.Name}}"
  },
  "default-address-pools": [
    {
      "base": "172.17.0.0/16",
      "size": 24
    }
  ]
}
